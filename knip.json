  "entry": ["src/main.tsx", "vite.config.ts", "vitest.config.ts"],
  "project": ["src/**/*.{ts,tsx}"],
  "ignore": ["src/components/ui/**", "src/test/**", "**/index.ts", "src/lib/platform-strings.ts"],
